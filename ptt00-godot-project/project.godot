; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "WaiterSetup",
"language": "GDScript",
"path": "res://waiter_setup.gd"
} ]
_global_script_class_icons={
"WaiterSetup": ""
}

[application]

config/name="waiter godot example project (ptt00)"
run/main_scene="res://main.tscn"
boot_splash/image="res://waiter/waiter100.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0.207843, 0.254902, 0.282353, 1 )
config/icon="res://waiter/waiter100.png"

[display]

window/size/width=400
window/size/height=250

[gui]

common/drop_mouse_on_gui_input_disabled=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0.207843, 0.254902, 0.282353, 1 )
environment/default_environment="res://default_env.tres"
